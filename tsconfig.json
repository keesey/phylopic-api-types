{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "es6"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmit": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "target": "es2016"
    },
    "files": [
        "src/index.ts"
    ],
    "paths": {
        "phylopic-api-types": "."
    }
}